    validFunction = (rule, value, callback) => {
         const reg = /^(?![0-9]+$)(?![a-zA-Z]+$)[0-9A-Za-z]{8,16}$/ig //长度必须大于6位
        if (value && !value.match(reg)) {
            callback('必须包含数字和字母！')
        }
        callback()
    }
    
    rules: [
           {
              validator: this.validFunction
           },
                {
                        required: true,
                        message: '请输入标题 !',
           },
        ],
