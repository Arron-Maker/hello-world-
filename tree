// 遍历修改树字段
export const traverseTree = org => {
  if (!org) return {};
  const haveChildren = org?.childAreaInfo?.length;
  return {
    ...org,
    label: org.areaName,
    title: org.areaName,
    value: org.areaCode,
    children: haveChildren ? org.childAreaInfo.map(i => traverseTree(i)) : [],
  };
};
