{
    title: '权限',
    dataIndex: 'roleid',
    render: (v) => {
        switch (v) {
            case 1: return <Tag> 管理员 </Tag>
            case 2:  return <Tag> 超级管理员 </Tag>
            case 3: return <Tag> 普通用户 </Tag>
            default: return <Tag>none</Tag>
        }
    }
}

//图片异常显示默认图片
{
  title: '图片',
  dataIndex: 'bannerUrl',
  render: item => <img src={item} onError={this.err} />,
},
